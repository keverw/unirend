{
  "name": "unirend",
  "version": "0.0.4",
  "type": "module",
  "description": "A lightweight toolkit for working with both SSG (Static Site Generation) and SSR (Server-Side Rendering) in your Vite + React projects.",
  "exports": {
    "./client": {
      "types": "./dist/client/client.d.ts",
      "import": "./dist/client/client.js",
      "require": "./dist/client/client.cjs"
    },
    "./server": {
      "types": "./dist/server/server.d.ts",
      "import": "./dist/server/server.js",
      "require": "./dist/server/server.cjs"
    },
    "./router-utils": {
      "types": "./dist/router-utils/router-utils.d.ts",
      "import": "./dist/router-utils/router-utils.js",
      "require": "./dist/router-utils/router-utils.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/plugins.d.ts",
      "import": "./dist/plugins/plugins.js",
      "require": "./dist/plugins/plugins.cjs"
    },
    "./api-envelope": {
      "types": "./dist/api-envelope/api-envelope.d.ts",
      "import": "./dist/api-envelope/api-envelope.js",
      "require": "./dist/api-envelope/api-envelope.cjs"
    },
    "./starter-templates": {
      "types": "./dist/starter-templates/starter-templates.d.ts",
      "import": "./dist/starter-templates/starter-templates.js",
      "require": "./dist/starter-templates/starter-templates.cjs"
    },
    "./utils": {
      "types": "./dist/utils/utils.d.ts",
      "import": "./dist/utils/utils.js",
      "require": "./dist/utils/utils.cjs"
    }
  },
  "bin": {
    "unirend": "./dist/cli/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "sync-version": "bun run scripts/sync-version.ts",
    "check-deps": "bun run scripts/check-deps.ts",
    "build": "bun run sync-version && bun run check-deps && bun run update-docs && tsup",
    "test": "bun test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md,html}\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "update-docs": "markdown-toc-gen update README.md && bun run scripts/update-readme-version.ts && markdown-toc-gen update changelog.md && find docs -type f -name '*.md' -print0 | xargs -0 -n1 markdown-toc-gen update && bun run format:docs",
    "format:docs": "prettier --write README.md changelog.md \"docs/**/*.md\"",
    "ssg-dev": "cd demos/ssg && vite",
    "ssg-build-client": "cd demos/ssg && vite build --outDir build/client --base=/ --ssrManifest",
    "ssg-build-server": "cd demos/ssg && vite build --outDir build/server --ssr src/entry-ssg.tsx",
    "ssg-build": "bun run ssg-build-client && bun run ssg-build-server",
    "ssg-generate": "cd demos/ssg && bun run generate.ts",
    "ssg-build-and-generate": "bun run ssg-build && bun run ssg-generate",
    "ssg-serve": "cd demos/ssg && bun run serve.ts",
    "ssr-dev": "cd demos/ssr && vite",
    "ssr-build-client": "cd demos/ssr && vite build --outDir build/client --base=/ --ssrManifest",
    "ssr-build-server": "cd demos/ssr && vite build --outDir build/server --ssr src/entry-server.tsx",
    "ssr-build": "bun run ssr-build-client && bun run ssr-build-server",
    "ssr-serve-dev": "cd demos/ssr && bun run serve.ts dev",
    "ssr-serve-prod": "cd demos/ssr && bun run serve.ts prod",
    "ssr-build-and-serve-prod": "bun run ssr-build && bun run ssr-serve-prod",
    "api-demo": "bun run demos/api-server-demo.ts",
    "api-static-demo": "bun run demos/api-static-content-demo.ts",
    "ws-demo": "bun run demos/ws-server-demo.ts",
    "run-dist-cli": "bun ./dist/cli/cli.js",
    "run-dev-cli": "bun run src/cli.ts"
  },
  "keywords": [
    "ssg",
    "ssr",
    "vite",
    "react",
    "typescript"
  ],
  "author": "Kevin Whitman (https://github.com/keverw)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keverw/unirend"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.2.13",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.2",
    "@types/semver": "^7.7.1",
    "@types/tr46": "^5.0.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "markdown-toc-gen": "^1.2.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-helmet-async": "^2.0.0",
    "react-router": "^7.0.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/middie": "^9.0.0",
    "@fastify/multipart": "^9.0.0",
    "@fastify/websocket": "^11.2.0",
    "cheerio": "^1.1.0",
    "fastify": "^5.0.0",
    "punycode": "^2.3.1",
    "range_check": "^4.1.0",
    "semver": "^7.7.3",
    "sort-package-json": "^3.4.0",
    "tldts": "^7.0.12",
    "tr46": "^5.1.1",
    "ulid": "^3.0.1"
  }
}
